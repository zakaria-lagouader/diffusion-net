   "source": [
    "import numpy as np\n",
    "import glob\n",
    "import os"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "bins = np.linspace(0, 1, 32)\n",
    "val_files = glob.glob('./*.val')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "for file in val_files:\n",
    "    data = np.digitize(np.loadtxt(file), bins)\n",
    "    np.savetxt(os.path.basename(file).replace('.val', '.txt'), data, fmt='%d')"
   ]
  },
